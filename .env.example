# Copy this file to .env and update the values before running CDK commands locally.
# AWS credentials are required so the AWS CDK can deploy CloudFormation stacks on your behalf.
# If you use IAM roles or SSO, you can leave the keys blank and rely on AWS_PROFILE instead.

# Static access key credentials (leave empty when using AWS SSO or role assumption)
export AWS_ACCESS_KEY_ID=
export AWS_SECRET_ACCESS_KEY=
export AWS_SESSION_TOKEN=

# The default AWS region used by CDK commands
export AWS_REGION=us-east-1

# Optional named AWS profile (mirrors ~/.aws/config) â€“ useful when keys are not provided above
export AWS_PROFILE=
